{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "quotes": ["error", "single"],
    "react/prop-types": 0,
    "react/react-in-jsx-scope": 0,
    "no-unused-vars": [
      "warn",
      { "vars": "all", "args": "all", "ignoreRestSiblings": false }
    ]
  }
}
